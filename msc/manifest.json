{
    "manifest_version": 3,
    "name": "Block Mouse Buttons",
    "version": "1.1",
    "description": "block back-froward mouse buttons",
    "permissions": ["scripting"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  